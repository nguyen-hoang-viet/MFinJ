<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href= "/css/showDB.css">
</head>

<body>
    <div class="container">
        <a href="/">Trang chá»§</a>
    </div>

    <div class="table-container">
        <table>

            <tr>
                <th>Content</th>
                <th>Link</th>
                <th>Tag</th>
            </tr>
            

            <% listJobs.forEach(function(job) { %>
                <tr>
                    <td class="content">
                        <pre><%= job.content %></pre>
                    </td>
                    <td>
                        <a href="<%= job.url %>" target="_blank"><%= job.url %></a>
                    </td>
                    <td>
                        <%= job.job%>
                    </td>
                </tr>
            <% }) %>

        </table>
    </div>

</body>
</html>